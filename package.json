  "homepage": "https://github.com/ryoctrl/TypeScriptTemplate#readme",
  "devDependencies": {
    "@types/moment": "^2.13.0",
    "@types/node": "14",
    "@types/puppeteer": "^5.4.3",
    "@types/sharp": "^0.26.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "6.15.0",
    "husky": "^4.3.0",
